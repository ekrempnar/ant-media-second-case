{
    "reporter": "cypress-multi-reporters",
    "reporterOptions": { 
      "reporterEnabled": "mochawesome",
      "mochawesomeReporterOptions": {
        "reportDir": "cypress/reports/mocha",
        "quite": true,
        "overwrite": false,
        "html": false,
        "json": true
      },
      "charts": true,
      "reportPageTitle": "My Test Suite",
      "embeddedScreenshots": true,
      "inlineAssets": true,
      "chromeSecurity": false
    },
    "video": false,

  
    "env": {
      "browserPermissions": {
        "notifications": "allow",
        "geolocation": "allow",
        "camera": "allow",
        "microphone": "allow",
        "images": "allow",
        "javascript": "allow",
        "popups": "allow",
        "plugins": "allow",
        "cookies": "allow"
      }
    
    }
}
